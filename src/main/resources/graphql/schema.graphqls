#graphqls for graphql scema

type Query {

    #Company Queries
    getCompany(id: ID!): Company

    #Appointment Queries
    getAppointment(id: ID!): Appointment
}

type Mutation {

    #Company Mutations
    createCompany(
        companyName: String!,
        ownerEmail: String!,
        ownerName: String!,
        ownerPassword: String!
    ): String

    editCompany(
        companyName: String,
        description: String,
        businessType: String
    ): String

    deleteCompany: String

    #Appointment Mutations
}

type Company {
    id: ID
    name: String
    description: String
    businessType: String
    memberEmails: [String]
    ownerEmail: String
}

type Appointment {
    id: ID
    from: DateTime
    to: DateTime
    companyI: ID
    clientID: ID
    assigneeID: ID
    # Select Prompt â†’ f.E. medical industry: Untersuchung, Operation
    description: String
    location: String
    AppointmentStatus: String
}

type DateTime {
    year: Int
    month: Int
    day: Int
    hour: Int
    minute: Int
    second: Int
}